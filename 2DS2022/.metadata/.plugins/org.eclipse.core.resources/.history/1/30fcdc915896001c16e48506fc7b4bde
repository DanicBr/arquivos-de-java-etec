public class TiposPrimitivos{
	
	public static void main(String args[]){
		char sexo = 'f';
		byte idade = 89;
		short codigo = 256; 
		float nota = 9.4f;
		int alunos = 100, classes = 10; 
		long habitantes = 9050100;
		double dolar = 2.62; 
		boolean alternativa = false ;
		
		System.out.println("sexo: " + sexo +" \nidade: "+ idade + " \nCodigo: " + codigo);
		System.out.println("nota: "+ nota + " \nalunos: " + alunos);
		System.out.println("habitantes: " + habitantes + " \ndolar: "+ dolar + " \nalternativas: " + habitantes);
		
		
		
	}
}
